<Window x:Class="Desafio_Agenda.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desafio_Agenda"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button x:Name="BtnContato" Content="Contato" Grid.Row="1" Click="BtnContato_Click"/>
        <Button x:Name="BtnCompromisso" Content="Compromissos" Grid.Row="3" Click="BtnCompromisso_Click"/>
        <Button x:Name="BtnFinalizar" Content="Finalizar Aplicação" Grid.Row="5" Click="BtnFinalizar_Click"/>




    </Grid>
</Window>
